USE EXERCICIO1_DML;
SELECT NOME, timestampdiff(YEAR, DATA_NASC,NOW()) AS IDADE
FROM PARTICIPANTES 
WHERE timestampdiff(YEAR, DATA_NASC,NOW()) > 40;
